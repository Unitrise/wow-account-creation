{
  "name": "wow-client",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "server:build": "tsc -p tsconfig.server.json",
    "server:start": "node dist/server/server.js",
    "server:dev": "tsc -p tsconfig.server.json --watch & nodemon dist/server/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "start": "npm run build && npm run pm2:start",
    "pm2:start": "npx pm2 start ecosystem.config.js",
    "pm2:stop": "npx pm2 stop ecosystem.config.js",
    "pm2:restart": "npx pm2 restart ecosystem.config.js",
    "pm2:delete": "npx pm2 delete ecosystem.config.js",
    "pm2:logs": "npx pm2 logs",
    "pm2:monit": "npx pm2 monit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/i18next": "^12.1.0",
    "@types/react-i18next": "^7.8.3",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.8.4",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.1",
    "i18next": "^24.2.3",
    "jsbn": "^1.1.0",
    "mysql2": "^3.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-router-dom": "^7.4.1",
    "stream-browserify": "^3.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jsbn": "^1.2.33",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "memfs": "^4.17.0",
    "nodemon": "^3.1.9",
    "path-browserify": "^1.0.1",
    "pm2": "^5.3.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
